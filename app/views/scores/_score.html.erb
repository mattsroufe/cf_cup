<div id="<%= dom_id score %>">
  <p>
    <strong>Match:</strong>
    <%= score.match_id %>
  </p>

  <p>
    <strong>Hole:</strong>
    <%= score.hole_id %>
  </p>

  <p>
    <strong>Player:</strong>
    <%= score.player_id %>
  </p>

  <p>
    <strong>Score:</strong>
    <%= score.score %>
  </p>

</div>
