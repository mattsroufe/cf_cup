<div id="<%= dom_id hole %>">
  <p>
    <strong>Course:</strong>
    <%= hole.course_id %>
  </p>

  <p>
    <strong>Number:</strong>
    <%= hole.number %>
  </p>

  <p>
    <strong>Par:</strong>
    <%= hole.par %>
  </p>

  <p>
    <strong>Meters:</strong>
    <%= hole.meters %>
  </p>

  <p>
    <strong>Stroke:</strong>
    <%= hole.stroke %>
  </p>

</div>
